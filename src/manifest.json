{
  "manifest_version": 2,
  "name": "Hifitorget prisjakt",
  "short_name": "HifiJakt",
  "version": "0.22",
  "description": "",
  "author": "crash007",
  "permissions": [
    "https://*.google.se/",
    "http://*.google.se/",
    "https://*.prisjakt.nu/",
    "http://*.prisjakt.nu/"
  ],
  
  "icons": {
    "72": "icons/icon-72x72.png",
    "96": "icons/icon-96x96.png",
    "128": "icons/icon-128x128.png"
  },
  "background": {
    "scripts": [
      "libs/jquery-3.3.1.min.js",    
      "background.js"
    ]
  },
  "browser_action": {
   
    "default_icon": {
      "72": "icons/icon-72x72.png",
      "96": "icons/icon-96x96.png",
      "128": "icons/icon-128x128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.hifitorget.se/*",
        "http://*.hifitorget.se/"
	
      ],
      "js": [
        "libs/jquery-3.3.1.min.js",    
        "frontend.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{182738d2-c07b-4c5d-0797-bf42adbb9c88}"
    }
  }


}

